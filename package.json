		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scrollspy": "^3.4.3",
		"remix": "^1.0.6",
		"remix-validated-form": "^3.4.2",
		"zod": "^3.11.6",
		"zod-form-data": "^1.0.1"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.0.6",
